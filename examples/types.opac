module Main is
    use std.Io;
    exports main/0;

    type Person
        name;
        age;
    end

    fun formatPerson (person) =
        Io.format("{{ name: '{0}', age: {1} }}", [person.name, person.age]);

    fun main () = do
        let person = Person("Jacob", 42);
        Io.println(formatPerson(person));
    end
end
